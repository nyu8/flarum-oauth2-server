{
    "name": "nyu8/flarum-oauth2-server",
    "version": "1.0.0",
    "description": "Turn your Flarum user system into an OAuth2 server.",
    "type": "flarum-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NYU8\\FlarumOAuth2Server\\": "src/"
        }
    },
    "authors": [
        {
            "name": "NYU8",
            "email": "dev@nyu8.com"
        }
    ],
    "require": {
        "flarum/core": "^1.0"
    },
    "extra": {
        "flarum-extension": {
            "title": "OAuth2 Server",
            "icon": {
                "name": "fas fa-server",
                "backgroundColor": "#eb5424",
